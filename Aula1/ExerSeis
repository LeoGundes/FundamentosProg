public class ExerSeis
{
    public static void main (String args[]){
        
        double altura = 6;
        double largura = 4;
        double comprimento = 9;
        
        double area = altura * largura;
        System.out.println("√Årea do quadrado: " + area);
        
        double volume = area * comprimento;
        System.out.println("Volume do quadrado: " + volume);
    }
}
