/*Faça um programa para calcular a área livre, em metros quadrados e
porcentagem, de um retângulo maior que possui um retângulo menor no seu
interior.*/


public class ExerOito{
    public static void main(String[] args){
        
        //dimensões do quadrado maior
        double larguraMaior = 10.0;
        double comprimentoMaior = 20.0;
        
        //dimensões do quadrado menor
        double larguraMenor = 4.0;
        double comprimentoMenor = 8.0;
        
        //calculando as areas
        double areaTotal = larguraMaior * comprimentoMaior;
        double areaInterno = larguraMenor * comprimentoMenor;
        double areaLivre = areaTotal - areaInterno;
        
        //calculando a porcentagem
        double porcentagemLivre = (areaLivre / areaTotal) * 100;
        
        System.out.println("Área livre: " + areaLivre + " metros quadrados.");
        
        System.out.println("Porcentagem livre: " + porcentagemLivre + "%");
        
    }
}
